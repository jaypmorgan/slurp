(library tidyverse)

(defparam x
  (rnorm 100 0 1))

(print (* x 2))

(print (mean (^ x 10)))

(print
  (map [1 2 3 4 5] (lambda (x) (* x 10))))

(defparam weird-name? TRUE)
(print weird-name?)

(defun my-cool-function (num-samples mean sd)
  (rnorm num-samples :mean mean :sd sd))

(print (my-cool-function 10 10 10))

(+ 1 2 3 4 5)